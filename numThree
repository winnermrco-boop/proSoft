using System;
using System.Linq;

class Solution
{
    static void Main(string[] args)
    {
        double userLon = Convert.ToDouble(Console.ReadLine().Replace(',', '.'));
        double userLat = Convert.ToDouble(Console.ReadLine().Replace(',', '.'));
        int N = int.Parse(Console.ReadLine());
        
        string closestName = "";
        double minDistance = double.MaxValue;
        
        for (int i = 0; i < N; i++)
        {
            string[] defibData = Console.ReadLine().Split(';');
            
            double defibLon = Convert.ToDouble(defibData[4].Replace(',', '.'));
            double defibLat = Convert.ToDouble(defibData[5].Replace(',', '.'));
            
            double x = (defibLon - userLon) * Math.Cos((userLat + defibLat) / 2);
            double y = (defibLat - userLat);
            double distance = Math.Sqrt(x*x + y*y) * 6371;
            
            if (distance < minDistance)
            {
                minDistance = distance;
                closestName = defibData[1];
            }
        }

        Console.WriteLine(closestName);
    }
}
